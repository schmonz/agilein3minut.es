ServerName	agilein3minut.es
ServerAdmin	webmaster@agilein3minut.es
Listen		127.0.0.1:30096
DocumentRoot	/home/schmonz/sites/agilein3minut.es/www/html
ErrorLog	/home/schmonz/sites/agilein3minut.es/www/logs/errors

LoadModule alias_module lib/httpd/mod_alias.so
	ScriptAlias /cgi/ /home/schmonz/sites/agilein3minut.es/www/cgi/
	Redirect permanent /index.rss https://agilein3minut.es/archive/index.rss

LoadModule autoindex_module lib/httpd/mod_autoindex.so
	IndexIgnore */.??* *~ *# */HEADER* */README* */RCS */CVS

LoadModule authz_core_module lib/httpd/mod_authz_core.so
	<Location />
		Require all granted
	</Location>

LoadModule cgid_module lib/httpd/mod_cgid.so
	ScriptSock /home/schmonz/sites/agilein3minut.es/www/run/cgid.sock

LoadModule dir_module lib/httpd/mod_dir.so
	DirectoryIndex index.html

LoadModule log_config_module lib/httpd/mod_log_config.so
	LogFormat "%a %l %u %t \"%r\" %>s %b \"%{Referer}i\" \"%{User-Agent}i\"" combined_for_proxied_site
	CustomLog /home/schmonz/sites/agilein3minut.es/www/logs/access combined_for_proxied_site

LoadModule mpm_prefork_module lib/httpd/mod_mpm_prefork.so
	PIDFile /home/schmonz/sites/agilein3minut.es/www/run/apache.pid

LoadModule remoteip_module lib/httpd/mod_remoteip.so
	RemoteIPHeader X-Forwarded-For
	RemoteIPInternalProxy 127.0.0.1

LoadModule unixd_module lib/httpd/mod_unixd.so
